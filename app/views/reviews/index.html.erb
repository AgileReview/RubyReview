<h1>Listing reviews</h1>

<table>
  <tr>
    <th>Review Of</th>
    <th>Due By</th>
    <th></th>
  </tr>

<% @reviews.each do |review| %>
  <tr>
    <td><%= review.TeamMember.name %></td>
    <td><%= review.dueDate %></td>
    <td><%= link_to 'Respond', new_evaluation_path(:review_id => review.id) %></td>
  </tr>
<% end %>
</table>
<br />
<%= link_to 'New Review', new_review_path %>